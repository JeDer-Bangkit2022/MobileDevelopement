<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.JeDerV1" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="android:fontFamily">@font/varela_round</item>
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="colorOnPrimary">@color/black</item>
        <item name="backgroundColor">@color/primaryColor</item>
        <item name="colorOnBackground">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondaryColor</item>
        <item name="colorSecondaryVariant">@color/secondaryDarkColor</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/primaryDarkColor</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.JeDerV1.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="android:fontFamily">@font/varela_round</item>
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="colorOnPrimary">@color/black</item>
        <item name="backgroundColor">@color/primaryColor</item>
        <item name="colorOnBackground">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondaryColor</item>
        <item name="colorSecondaryVariant">@color/secondaryDarkColor</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/primaryDarkColor</item>
        <!-- Customize your theme here. -->
    </style>



</resources>